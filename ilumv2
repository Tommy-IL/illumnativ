-- local scripts

local function JFVLTW_fake_script() -- ToggleSecond.LocalScript 
	local script = Instance.new('LocalScript', ToggleSecond)

	local toggle = script.Parent
	local second = script.Parent.Parent.Second
	
	
	toggle.MouseButton1Down:connect(function()
		second.Visible = not second.Visible
	end)
end
coroutine.wrap(JFVLTW_fake_script)()
