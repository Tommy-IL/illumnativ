RainbowSaber.TextColor3 = Color3.fromRGB(255, 255, 255)
RainbowSaber.TextSize = 21.000
RainbowSaber.TextWrapped = true
RainbowSaber.MouseButton1Down:connect(function()
	local colors = {"Blue", "Green", "Yellow", "Cyan", "Purple", "White", "BlackGreen", "Orange", "BlackBlue", "BlackRed", "BlackYellow", "BlackRainbow", "BlackPink", "BlackPurple"}

	while true do
		for _,chosenColor in pairs(colors) do
			game.ReplicatedStorage.Remotes['CrystalChange']:FireServer(chosenColor)
			wait(0.1)
		end
	end
end)

